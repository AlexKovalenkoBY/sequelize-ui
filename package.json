{
  "name": "sequelize-ui",
  "version": "0.1.0",
  "description": "GUI for designing relational database schema and exporting as formatted Sequlize template.",
  "homepage": "http://sequelizeui.herokuapp.com",
  "bugs": {
    "url": "https://github.com/tomjschuster/sequelize-ui/issues",
    "email": "tomjschuster@gmail.com"
  },
  "license": "ISC",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomjschuster/sequelize-ui"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "./node_modules/standard/bin/cmd.js",
    "format": "./node_modules/standard/bin/cmd.js --fix",
    "clean": "rimraf dist",
    "build:dev": "NODE_ENV=development && npm run clean && webpack",
    "build:dev-watch": "NODE_ENV=development && npm run clean && webpack --watch",
    "start:dev": "NODE_ENV=development && npm run build:dev-watch & nodemon server",
    "build:prod": "NODE_ENV=production && npm run clean && webpack --config ./webpack.prod.js",
    "start": "NODE_ENV=production && node server",
    "heroku-postbuild": "npm run build:prod",
    "deploy": "NODE_ENV=production && npm run build:prod && node deploy"
  },
  "keywords": [
    "sequelize",
    "orm",
    "sql",
    "schema",
    "model",
    "gui"
  ],
  "nodemonConfig": {
    "ignore": [
      "src/*",
      "assets/*",
      "dist/*"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.15.2",
    "case": "^1.5.4",
    "chalk": "^2.3.2",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "file-saver": "^1.3.3",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.4",
    "jszip": "^3.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "modules-values-extract": "^1.0.3",
    "morgan": "^1.7.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.6.2",
    "scp2": "^0.5.0",
    "semantic-ui-less": "^2.3.1",
    "semantic-ui-react": "^0.79.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "nodemon": "^1.14.12",
    "standard": "^11.0.0"
  },
  "engines": {
    "node": "9.5.0"
  }
}
