{
  "name": "sequelize-ui",
  "version": "0.2.0",
  "description": "Browser-based GUI for previewing and generating Sequelize project files.",
  "homepage": "https://sequelizeui.app",
  "bugs": {
    "url": "https://github.com/tomjschuster/sequelize-ui/issues",
    "email": "tomjschuster@gmail.com"
  },
  "license": "ISC",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomjschuster/sequelize-ui"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=development webpack --mode=development",
    "watch": "NODE_ENV=development webpack --mode=development --watch",
    "start": "npm run build &&   NODE_ENV=development live-server ./dist & npm run watch",
    "deploy": "rimraf dist && NODE_ENV=production webpack --mode=production"
  },
  "keywords": [
    "sequelize",
    "orm",
    "sql",
    "gui",
    "code",
    "generator",
    "codegen",
    "code generator"
  ],
  "browserslist": [
    "defaults"
  ],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "Blob",
      "localStorage",
      "location"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "case": "^1.6.3",
    "copy-to-clipboard": "^3.3.1",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "html-webpack-plugin": "^5.0.0-alpha.2",
    "inflection": "^1.12.0",
    "jszip": "^3.5.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss-loader": "^4.1.0",
    "prismjs": "^1.22.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sitemap-webpack-plugin": "^1.0.0",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0",
    "xregexp": "^4.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "prettier": "^2.2.1",
    "standard": "^16.0.3"
  },
  "engines": {
    "node": ">=6.0.0 < 12.0"
  }
}
