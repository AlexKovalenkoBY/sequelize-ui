{
  "name": "sequelize-ui",
  "version": "0.1.0",
  "description": "GUI for designing relational database schema and exporting as formatted Sequlize template.",
  "homepage": "http://sequelizeui.herokuapp.com",
  "bugs": {
    "url": "https://github.com/tomjschuster/sequelize-ui/issues",
    "email": "tomjschuster@gmail.com"
  },
  "license": "ISC",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomjschuster/sequelize-ui"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "./node_modules/standard/bin/cmd.js",
    "format": "./node_modules/standard/bin/cmd.js --fix",
    "clean": "rimraf dist",
    "build:dev": "NODE_ENV=development && npm run clean && webpack",
    "build:dev-watch": "NODE_ENV=development && npm run clean && webpack --watch",
    "start:dev": "NODE_ENV=development && npm run build:dev-watch & nodemon server",
    "build:prod": "NODE_ENV=production && npm run clean && webpack --config ./webpack.prod.js",
    "start": "NODE_ENV=production && node server",
    "heroku-postbuild": "npm run build:prod",
    "deploy": "NODE_ENV=production && npm run build:prod && node deploy"
  },
  "keywords": [
    "sequelize",
    "orm",
    "sql",
    "schema",
    "model",
    "gui"
  ],
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.2.6",
    "axios": "^0.16.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.15.2",
    "brace": "^0.10.0",
    "case": "^1.5.4",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "express": "^4.14.0",
    "file-saver": "^1.3.3",
    "html-webpack-plugin": "^2.30.1",
    "jszip": "^3.1.5",
    "modules-values-extract": "^1.0.3",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.4",
    "react-router": "3.x",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "redux-localstorage": "^1.0.0-rc5",
    "redux-localstorage-filter": "^0.1.1",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.6.2",
    "scp2": "^0.5.0",
    "style-loader": "^0.20.1",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "nodemon": "^1.14.12",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": "9.5.0"
  }
}
