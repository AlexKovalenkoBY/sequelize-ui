{
  "name": "sequelize-ui",
  "version": "0.2.0",
  "description": "Browser-based GUI for previewing and generating Sequelize project files.",
  "homepage": "https://sequelizeui.app",
  "bugs": {
    "url": "https://github.com/tomjschuster/sequelize-ui/issues",
    "email": "tomjschuster@gmail.com"
  },
  "license": "ISC",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomjschuster/sequelize-ui"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=development webpack --mode=development",
    "watch": "NODE_ENV=development webpack --mode=development --watch",
    "start": "npm run build &&   NODE_ENV=development live-server ./dist & npm run watch",
    "deploy": "rimraf dist && NODE_ENV=production webpack --mode=production"
  },
  "keywords": [
    "sequelize",
    "orm",
    "sql",
    "gui",
    "code",
    "generator",
    "codegen",
    "code generator"
  ],
  "browserslist": [
    "defaults"
  ],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "Blob",
      "localStorage",
      "location"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "case": "^1.6.1",
    "copy-to-clipboard": "^3.2.0",
    "copy-webpack-plugin": "^6.1.0",
    "core-js": "^3.2.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "file-saver": "^2.0.2",
    "html-webpack-plugin": "^3.2.0",
    "inflection": "^1.12.0",
    "jszip": "^3.2.1",
    "live-server": "^1.2.1",
    "lodash": ">=4.17.19",
    "mini-css-extract-plugin": "^0.6.0",
    "minimist": "^1.2.5",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sitemap-webpack-plugin": "^0.6.0",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "prettier": "^1.17.1",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">=6.0.0 < 12.0"
  }
}
