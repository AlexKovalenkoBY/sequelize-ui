{
  "name": "sequelize-ui",
  "version": "0.2.0",
  "description": "Browser-based GUI for previewing and generating Sequelize project files.",
  "homepage": "https://sequelizeui.app",
  "bugs": {
    "url": "https://github.com/tomjschuster/sequelize-ui/issues",
    "email": "tomjschuster@gmail.com"
  },
  "license": "ISC",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomjschuster/sequelize-ui"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=development webpack --mode=development",
    "watch": "NODE_ENV=development webpack --mode=development --watch",
    "start": "npm run build && NODE_ENV=development live-server ./dist & npm run watch",
    "deploy": "rimraf dist && NODE_ENV=production webpack --mode=production",
    "start.ts": "webpack-dev-server --hot --open --overlay --config webpack.config.ts",
    "build.ts": "webpack --mode production --config webpack.config.ts",
    "types.check": "tsc",
    "lint": "tslint --project tsconfig.json",
    "format": "prettier 'ts/**/*.{ts,tsx}' --write",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "sequelize",
    "orm",
    "sql",
    "gui",
    "code",
    "generator",
    "codegen",
    "code generator"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/styled-components": "^4.4.2",
    "@types/webpack": "^4.41.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "case": "^1.6.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-to-clipboard": "^3.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.2.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "file-saver": "^2.0.2",
    "html-webpack-plugin": "^3.2.0",
    "inflection": "^1.12.0",
    "jszip": "^3.2.1",
    "live-server": "^1.2.1",
    "lodash": ">=4.17.13",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "robotstxt-webpack-plugin": "^5.0.0",
    "sitemap-webpack-plugin": "^0.6.0",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.10",
    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack-dev-server": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "webpack-dev-server": "^3.10.1"
  },
  "engines": {
    "node": ">=6.0.0 < 12.0"
  }
}
